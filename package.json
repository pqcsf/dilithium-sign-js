{
  "name": "dilithium-sign",
  "version": "1.0.0",
  "description": "The JS module of the post-quantum digital signature algorithm (Dilithium).",
  "main": "index.js",
  "scripts": {
    "bulid-dilithium2-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/dilithium/*.c -o ./kernel/n3_v1/wasmFile/dilithium2.js -D DILITHIUM_MODE=2",
	"bulid-dilithium3-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/dilithium/*.c -o ./kernel/n3_v1/wasmFile/dilithium3.js -D DILITHIUM_MODE=3",
	"bulid-dilithium5-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/dilithium/*.c -o ./kernel/n3_v1/wasmFile/dilithium5.js -D DILITHIUM_MODE=5",
	"bulid-dilithium2R-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/dilithium/*.c -o ./kernel/n3_v1/wasmFile/dilithium2-R.js -D DILITHIUM_MODE=2 -D DILITHIUM_RANDOMIZED_SIGNING=1",
	"bulid-dilithium3R-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/dilithium/*.c -o ./kernel/n3_v1/wasmFile/dilithium3-R.js -D DILITHIUM_MODE=3 -D DILITHIUM_RANDOMIZED_SIGNING=1",
	"bulid-dilithium5R-n3-v1": "emcc ./kernel/n3_v1/wasmapi.cc ./kernel/n3_v1/dilithium/*.c -o ./kernel/n3_v1/wasmFile/dilithium5-R.js -D DILITHIUM_MODE=5 -D DILITHIUM_RANDOMIZED_SIGNING=1",
	"bulid-all": "npm run bulid-dilithium2-n3-v1 && npm run bulid-dilithium3-n3-v1 && npm run bulid-dilithium5-n3-v1 && npm run bulid-dilithium2R-n3-v1 && npm run bulid-dilithium3R-n3-v1 && npm run bulid-dilithium5R-n3-v1"
},
  "repository": {
    "type": "git",
    "url": "https://github.com/pqcsf/dilithium-sign-js"
  },
  "keywords": [
    "PQC",
    "Cryptography",
    "Public-key cryptography"
  ],
  "author": "PQCSF",
  "license": "MIT",
  "homepage": "https://github.com/pqcsf/dilithium-sign-js"
}
